{
    "openapi": "3.0.0",
    "info": {
        "title": "Ticker APP API Documentation.",
        "version": "1.0.0"
    },
    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Register user",
                "description": "This can only be done by guest user.",
                "operationId": "registerUser",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "description": "Required *",
                                        "type": "string"
                                    },
                                    "email": {
                                        "description": "Required *",
                                        "type": "string"
                                    },
                                    "passowrd": {
                                        "description": "Required * min:8",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Bad request"
                    },
                    "500": {
                        "description": "Eexception"
                    },
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Login user",
                "description": "This can only be done by guest user.",
                "operationId": "loginUser",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Bad request"
                    },
                    "500": {
                        "description": "Eexception"
                    },
                    "200": {
                        "description": "successful operation"
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}